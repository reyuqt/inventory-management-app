# backend API endpoint
REACT_APP_API_URL=http://localhost:8000/api/v1

# Optional: Public URL (useful for deploying to a subdirectory)
# PUBLIC_URL=/myapp

# Feature Flags
REACT_APP_FEATURE_X_ENABLED=true
REACT_APP_FEATURE_Y_ENABLED=false

# Third-Party Service Keys (Ensure these are safe to expose)
REACT_APP_GOOGLE_MAPS_API_KEY=your_google_maps_api_key_here
REACT_APP_ANALYTICS_ID=your_analytics_id_here

# Authentication Redirect URLs
REACT_APP_LOGIN_REDIRECT_URL=http://localhost:3000/
REACT_APP_LOGOUT_REDIRECT_URL=http://localhost:3000/

# Any other environment-specific variables
REACT_APP_SUPPORT_EMAIL=support@example.com
HTTPS=true